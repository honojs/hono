{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "nano-jsx/lib",
    "target": "es2017",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "./dist",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
  ]
}